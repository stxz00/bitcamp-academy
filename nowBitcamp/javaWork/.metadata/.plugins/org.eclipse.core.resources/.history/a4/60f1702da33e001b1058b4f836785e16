package com.bit.day07;

public class Ex11 {
	public static void main(String[] args) {
		Machine obj;
		obj = new Audio();
		obj.on();
		obj.off();
		
		Tv obj1 = new Tv();
		obj1.on();
		obj1.work();
		obj1.off();
		Radio obj2 = new Radio();
		obj2.on();
		obj2.work();
		obj2.off();
		Audio obj3 = new Audio();
		obj3.on();
		obj3.work();
		obj3.off();
	}
}
