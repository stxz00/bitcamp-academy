package com.bit.day07;

class Ex02{


	public static void main(String[] args){
		com.bit.Ex102 ex2; //생성자 오류는 여기서 나옴
		ex2 = new com.bit.Ex102();
		System.out.println(ex2.su1);
		//System.out.println(ex2.su2); 디폴트 접근제한
		//System.out.println(ex2.su3); private 접근제한
		ex2.func01();
		ex2.func02(); // 메서드가 퍼블릭이므로 su2 디폴트 접근제한풀림
		ex2.func03(); // 메서드가 퍼블릭이므로 su2 private 접근제한풀림
	}
}